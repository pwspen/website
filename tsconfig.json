{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths":
    {
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@consts/*": ["src/consts/*"],
      "@layouts/*": ["src/layouts/*"],
      "@imgs/*": ["src/content/imgs/*"],
    }
  }
}